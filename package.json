{
  "name": "software-update-checker-cli",
  "version": "0.1.0",
  "author": "Adam Boe",
  "description": "A command line interface to check if local software installs have updates",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/aboe026/software-update-checker-cli.git"
  },
  "bugs": {
    "url": "https://github.com/aboe026/software-update-checker-cli/issues"
  },
  "homepage": "https://github.com/aboe026/software-update-checker-cli",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.14.4"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . && npm run prettier:check",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "start": "ts-node src/index.ts",
    "test": "npm run lint && npm run test:func",
    "test:func": "jest test/func --runInBand"
  },
  "dependencies": {
    "cli-progress": "3.8.2",
    "cli-table3": "0.6.0",
    "colors": "1.4.0",
    "fs-extra": "9.0.1",
    "inquirer": "7.3.3",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@types/cli-progress": "3.8.0",
    "@types/colors": "1.2.1",
    "@types/concat-stream": "1.6.0",
    "@types/fs-extra": "9.0.6",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.20",
    "@types/node-fetch": "2.5.8",
    "@typescript-eslint/parser": "4.14.0",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "concat-stream": "2.0.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  }
}
